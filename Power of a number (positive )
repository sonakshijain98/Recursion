
#include <iostream>
#include <vector>
#include <algorithm>
#include <string>
#include<bits/stdc++.h> 

using namespace std;
int power(int x,int n){
        if(n>=1)
         return x*(power(x,n-1));
        else
         return 1;
    }

int main()
{
    int x,n;
    cin>>x>>n;
    long long ans=power(x,n);
    cout<<ans;

    return 0;
}
